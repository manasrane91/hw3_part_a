<h1><%= @place["name"] %></h1>

<ul>
  <% for entry in @entries %>
    
    <h2>
        <%= entry["title"] %>
        
    </h2>

<li>
        <%= entry["posted_on"] %> 
    </li>

    <li>
        <%= entry["description"] %>
    </li>


  <% end %>
</ul>

<p><a href="/entries/new?place_id=<%= @place.id %>">New Post</a></p>


<p><a href="/places">Back to All Places</a></p>